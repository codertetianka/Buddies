{
  "expo": {
    "name": "buddies",
    "slug": "buddies",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png" 
    },
    "plugins": [
      [
        "expo-font",
      {
        "fonts": [
        "./assets/GT-Eesti/GT-Eesti-Display-Bold-Italic-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Display-Bold-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Display-Light-Italic-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Display-Light-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Display-Medium-Italic-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Display-Medium-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Display-Regular-Italic-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Display-Regular-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Display-Thin-Italic-Trial.otf"  ,
        "./assets/GT-Eesti/GT-Eesti-Display-Thin-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Display-UltraBold-Italic-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Display-UltraBold-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Display-UltraLight-Italic-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Display-UltraLight-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Text-Bold-Italic-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Text-Bold-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Text-Book-Italic-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Text-Book-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Text-Light-Italic-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Text-Light-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Text-Medium-Italic-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Text-Medium-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Text-Regular-Italic-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Text-Regular-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Text-Thin-Italic-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Text-Thin-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Text-ULight-Italic-Trial.otf",
        "./assets/GT-Eesti/GT-Eesti-Text-UltraLight-Trial.otf"
      ]
    }
   
      ],
      [
        "expo-camera",
      {
        "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
      }
      ]
      
      
    ]
  }
  
}
